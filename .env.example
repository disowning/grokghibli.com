# Hugging Face API Token (单个token示例)
HUGGING_FACE_TOKEN=hf_your_token_here

# Hugging Face API Tokens (多个token示例，使用逗号分隔)
# HUGGING_FACE_TOKENS=hf_token1,hf_token2,hf_token3

# Hugging Face API Tokens (多个token示例，使用索引)
# HUGGING_FACE_TOKEN_1=hf_token1
# HUGGING_FACE_TOKEN_2=hf_token2

# Admin Secret for token status API
ADMIN_SECRET=your_admin_secret_here

# Redis配置 - 使用反向代理的专用域名
REDIS_HOST=api-ip.grokghibli.com
# 无需端口配置，通过域名反向代理直接连接到Redis服务
REDIS_PASSWORD=your_redis_password

# MySQL配置 - 使用反向代理的专用域名
MYSQL_HOST=api-3.grokghibli.com
# 无需端口配置，通过域名反向代理直接连接到MySQL服务
MYSQL_USER=root
MYSQL_PASSWORD=your_mysql_password
MYSQL_DATABASE=grokghibli

# 图像处理服务域名
API_SERVER=api.grokghibli.com

# NextAuth配置
NEXTAUTH_URL=http://localhost:3000
NEXTAUTH_SECRET=your_nextauth_secret_key

# Google OAuth配置
GOOGLE_CLIENT_ID=your_google_client_id
GOOGLE_CLIENT_SECRET=your_google_client_secret 